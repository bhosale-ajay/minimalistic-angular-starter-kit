  "dependencies": {
    "@angular/common": "2.4.0",
    "@angular/compiler": "2.4.0",
    "@angular/core": "2.4.0",
    "@angular/forms": "2.4.0",
    "@angular/http": "2.4.0",
    "@angular/platform-browser": "2.4.0",
    "@angular/platform-browser-dynamic": "2.4.0",
    "@angular/router": "3.4.0",
    "core-js": "2.4.1",
    "rxjs": "5.0.1",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/core-js": "0.9.35",
    "@types/jasmine": "2.5.35",
    "@types/node": "6.0.45",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.1",
    "clean-webpack-plugin": "0.1.15",
    "codelyzer": "3.0.0-beta.0",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-webpack": "2.0.2",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.9.1",
    "style-loader": "0.13.2",
    "tslint": "4.5.1",
    "typescript": "2.2.1",
    "webpack": "2.2.0",
    "webpack-merge": "4.0.0"
  },
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --bail",
    "build:prod": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "lint": "tslint ./App/**/*.ts -t msbuild --type-check --project tsconfig.json",
    "test": "karma start config/karma.conf.js && npm run ccreport",
    "ccreport": "remap-istanbul -i config/coverage/coverage.json -o config/coverage/html-report -t html -e node_modules"
  }
}